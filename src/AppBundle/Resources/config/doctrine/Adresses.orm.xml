<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Adresses" table="Adresses">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="line1" type="string" column="line1" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line2" type="string" column="line2" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="line3" type="string" column="line3" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="city" type="string" column="city" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countryprovince" type="string" column="countryProvince" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="zipcode" type="integer" column="zipcode" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="otherdetails" type="string" column="otherDetails" length="2500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
