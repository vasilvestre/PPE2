<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Orders" table="Orders">
    <indexes>
      <index name="FK_Orders_id_Users" columns="id_Users"/>
      <index name="FK_Orders_id_Invoices" columns="id_Invoices"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datecreation" type="datetime" column="dateCreation" nullable="true"/>
    <many-to-one field="idInvoices" target-entity="Invoices" fetch="LAZY">
      <join-columns>
        <join-column name="id_Invoices" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsers" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="id_Users" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
