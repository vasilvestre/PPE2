<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="Users">
    <indexes>
      <index name="FK_Users_id_Adresses" columns="id_Adresses"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastname" type="string" column="lastName" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailadress" type="string" column="emailAdress" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="phonenumber" type="string" column="phoneNumber" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="firstname" type="string" column="firstName" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idAdresses" target-entity="Adresses" fetch="LAZY">
      <join-columns>
        <join-column name="id_Adresses" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
